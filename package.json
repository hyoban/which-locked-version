{
  "name": "which-locked-version",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@9.5.0",
  "description": "Find the version of the dependency you are using from your lock file.",
  "author": {
    "name": "Stephen Zhou",
    "url": "https://hyoban.cc",
    "email": "hi@hyoban.cc"
  },
  "license": "MIT",
  "homepage": "https://github.com/hyoban/which-locked-version#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hyoban/which-locked-version.git"
  },
  "bugs": "https://github.com/hyoban/which-locked-version/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bunchee",
    "dev": "bunchee --watch",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "prepare": "simple-git-hooks",
    "prepublishOnly": "nr build",
    "release": "release-it",
    "test": "vitest",
    "typecheck": "tsc"
  },
  "dependencies": {
    "yaml": "^2.4.5"
  },
  "devDependencies": {
    "@antfu/ni": "^0.22.0",
    "@types/node": "^20.14.11",
    "bunchee": "^5.2.2",
    "eslint": "^9.7.0",
    "eslint-config-hyoban": "3.0.0-beta.30",
    "lint-staged": "^15.2.7",
    "release-it": "^17.6.0",
    "release-it-pnpm": "^4.6.3",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.5.3",
    "vite": "^5.3.4",
    "vitest": "^2.0.3"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
